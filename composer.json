{
  "name": "floor12/yii2-module-ecommerce",
  "description": "Yii2 module to organize shop in yii2 project",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.13",
    "floor12/yii2-editmodal": "*",
    "floor12/yii2-summernote": "*",
    "floor12/yii2-module-files": "*",
    "kartik-v/yii2-widget-select2": "@dev",
    "kartik-v/yii2-widget-activeform": "@dev",
    "yii2mod/yii2-enum": "*",
    "yii2mod/yii2-ion-slider": "^1.2",
    "leandrogehlen/yii2-treegrid": "*",
    "voronkovich/sberbank-acquiring-client": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\ecommerce\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\ecommerce\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-files"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-summernote"
    }
  ]
}
